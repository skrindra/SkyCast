{
    "version": 2,
    "buildCommand": "pip install -r requirements.txt",
    "devCommand": "python manage.py runserver",
    "installCommand": "pip install -r requirements.txt",
    "framework": "django",
    "builds": [
        {
            "src": "weather_project/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "weather_project/wsgi.py"
        }
    ]
} 